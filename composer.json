{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "astrotomic/laravel-translatable": "^11.10",
        "axlon/laravel-postal-code-validation": "^3.3",
        "cloudinary-labs/cloudinary-laravel": "^2.0",
        "cohensive/oembed": "^0.16.0",
        "cviebrock/eloquent-sluggable": "^9.0",
        "doctrine/dbal": "3.3.7",
        "fakerphp/faker": "^1.19",
        "fruitcake/laravel-cors": "^2.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "0.6.6",
        "joelbutcher/socialstream": "^3.1",
        "kalnoy/nestedset": "^6.0",
        "kmlaravel/laravel-geographical-calculator": "^2.2",
        "kodeine/laravel-meta": "^2.1",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.2",
        "laravel/telescope": "^4.10",
        "laravel/tinker": "^2.5",
        "lunarphp/core": "^0.1",
        "lunarphp/scout-database-engine": "*",
        "maatwebsite/excel": "^3.1",
        "matthiasmullie/minify": "^1.3",
        "mcamara/laravel-localization": "^1.7",
        "mews/purifier": "dev-master",
        "nwidart/laravel-modules": "^9.0",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.3",
        "qirolab/laravel-themer": "^1.7",
        "spatie/laravel-collection-macros": "^7.12",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-translation-loader": "^2.7",
        "stripe/stripe-php": "^7.113",
        "tightenco/ziggy": "^1.0",
        "torann/geoip": "^3.0",
        "webwizo/laravel-shortcodes": "1.0.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "laravel/dusk": "^7.7",
        "laravel/envoy": "^2.7",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3",
        "protonemedia/inertiajs-events-laravel-dusk": "^1.3",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/GlobalFunctions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "getcandy/core"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "purifier": {
            "type": "vcs",
            "url": "https://github.com/bayubiz752/Purifier.git"
        }
    }
}
